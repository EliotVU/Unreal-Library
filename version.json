{
    "$schema": "https://raw.githubusercontent.com/dotnet/Nerdbank.GitVersioning/main/src/NerdBank.GitVersioning/version.schema.json",
    "version": "1.12.0",
    "publicReleaseRefSpec": [
        "^refs/heads/master",
        "^refs/tags/\\d+\\.\\d+\\.\\d+"
    ],
    "release": {
        "tagName": "{version}",
        "branchName": "Release-{version}"
    },
    "nuGetPackageVersion": {
        "semVer": 2
    },
    "cloudBuild": {
        "buildNumber": {
            "enabled": true
        }
    }
}
